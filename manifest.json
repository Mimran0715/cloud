{
    "manifest_version": 3,
    "name": "Cloud",
    "description": "A calmer browsing experience",
    "version": "1.0",
    "action": {
      "default_popup": "popup_menu.html"
    },
    "icons": {
          "16": "images/icon-16.png",
          "32": "images/icon-32.png",
          "48": "images/icon-48.png",
          "128": "images/icon-128.png"
    },
    "content_scripts": [
          {
            "js": ["scripts/content.js"],
            "matches": [
              "https://google.com/*"
            ]
          }
        ]
}